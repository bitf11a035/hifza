@model mywebproject.Models.Resume

@{
    ViewBag.Title = "DeleteItem1";
    Layout = "~/Views/Shared/_Layoutchicken.cshtml";
}
   <form action="/Admin/DeleteItem2/@ViewBag.X.res_Id">
<div id="content" class="float_r">
        	
            <h5>Delete product </h5>
            <div class="content_half float_l checkout">
                <img src="~/Files/@ViewBag.X.Path" alt="" width="20%" height="20%"/><br />
                <br />
		
             	 Product ID :  
                  <input type="text" id="nameofproduct"  name="nameofproduct" value="@ViewBag.X.res_Id" style="width:300px;"  />
                <br />
                <br />
                Product Name :  
                  <input type="text" id="n"  name="n" value="@ViewBag.X.Nic" style="width:300px;"  />
                <br />
                <br />
               Product quantity:
				<input type="text" id="q" name="q" value="@ViewBag.X.First_name" style="width:300px;"  />
                <br />
                <br />
              Product price:
                <input type="text"  id="p" name="p" value="@ViewBag.X.Last_name" style="width:300px;"  />
                <br />
                <br />
              
           <input type="submit" class="submit_btn" name="submit" id="submit" value="Delete product" />
                       
                 

            </div>
       </form>